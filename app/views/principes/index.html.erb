<% title "Principes" %>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% for principe in @principes %>
    <tr>
      <td><%=h principe.name %></td>
      <td><%= link_to "Edit", edit_principe_path(principe) %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Principe", new_principe_path %></p>
